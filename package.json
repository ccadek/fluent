    "@types/jest": "^25.2.1",
    "@types/js-combinatorics": "^0.5.31",
    "@types/mongodb-memory-server": "^2.3.0",
    "@types/multer": "^1.4.3",
    "@types/multer-s3": "^2.7.7",
    "@types/node": "^13.13.5",
    "@types/passport-http": "^0.3.8",
    "auto-changelog": "^2.0.0",
    "dotenv-cli": "^3.1.0",
    "jest": "^26.0.1",
    "npm-pack-zip": "^1.2.7",
    "prettier": "^2.0.5",
    "ts-jest": "^25.5.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  }
}
