{
  "name": "@goatlab/fluent",
  "version": "0.3.12",
  "description": "Readable query Interface & API generator for JS and Node",
  "homepage": "https://docs.goatlab.io",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prebuild": "npx rimraf dist",
    "prestart": "npx rimraf dist",
    "predev": "npx rimraf dist",
    "test": "npx jest --forceExit --detectOpenHandles",
    "dev": "npx tsc --watch",
    "tslint-check": "npx tslint-config-prettier-check ./tslint.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "update:packages": "ncu -u && npm install",
    "build": "npx tsc",
    "pack": "npx npm-pack-zip",
    "release": "npx release-it patch --no-npm.publish --ci",
    "release:patch": "npx release-it patch --no-npm.publish --ci",
    "release:minor": "npx release-it minor --no-npm.publish --ci",
    "release:mayor": "npx release-it mayor --no-npm.publish --ci ",
    "changelog": "npx auto-changelog --template changelog-template.hbs -p -u --commit-limit false",
    "changelog-debug": "npx auto-changelog --template changelog-template.hbs -p --template json --output changelog-data.json"
  },
  "keywords": [
    "goat",
    "fluent",
    "loopback",
    "nestjs"
  ],
  "author": "ignacio.cabrera@goatlab.io",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm test",
      "prepare-commit-msg": "exec < /dev/tty && npx git cz --hook",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run build"
    }
  },
  "release-it": {
    "src": {
      "commitMessage": "ci: CI-1 [Release] v%s"
    },
    "git": {
      "requireBranch": "master",
      "commitArgs": [
        "--no-verify"
      ]
    },
    "hooks": {
      "before:init": [
        "npm run build",
        "npm run pack"
      ],
      "after:bump": "npm run changelog",
      "after:release": "echo Successfully released ${name} v${version} to ${repo.repository}."
    },
    "github": {
      "release": true,
      "assets": [
        "./*.zip"
      ]
    }
  },
  "auto-changelog": {
    "commitLimit": false,
    "unreleased": true,
    "issueUrl": "https://goatlab.atlassian.net/browse/{id}",
    "replaceText": {
      "[Ff]eature:": "",
      "[Ff]ix:": "",
      "[Bb]reak:": "",
      "([A-Z]+-\\d+)": "[$1](https://goatlab.atlassian.net/browse//$1) - "
    },
    "includeBranch": [
      "master"
    ]
  },
