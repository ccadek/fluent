{
  "name": "@goatlab/goat-fluent",
  "version": "1.0.2",
  "description": "Readable query Interface for Local and Remote storage providers",
  "main": "lib/@goatlab/goat-fluent.min.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "rm -rf lib/* && webpack --env build",
    "rebuild": "rm -rf node_modules && npm install && npm run build",
    "dev": "rm -rf lib/* && webpack --progress --colors --watch --env dev",
    "test": "mocha-webpack --colors --recursive -w --timeout 100000 --webpack-config test/config/webpack.config.test.js --require mock-local-storage --require test/config/setup.js --exit src/**/**/*.spec.js --exit ",
    "test:watch": "mocha-webpack --webpack-config webpack.config.test.js --watch --glob '*.spect.js' --recursive test"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/goatlabs/goat-fluent.git"
  },
  "keywords": [
    "form.io",
    "js",
    "cordova",
    "browser"
  ],
  "author": "Ignacio Cabrera",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/goatlabs/goat-fluent.git/issues"
  },
  "homepage": "https://gitlab.com/goatlabs/goat-fluent.git#readme",
  "dependencies": {
    "@stamp/compose": "^1.0.2",
    "@stamp/it": "^1.1.0",
    "@stamp/privatize": "^1.0.3",
    "stampit": "^4.2.0"
  }
}
