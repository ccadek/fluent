{
  "name": "@goatlab/sockets-pusherrn",
  "version": "1.1.8-alpha.0",
  "description": "Sockets connector for Pusher React Native",
  "main": "lib/@goatlab/sockets-pusherrn.min.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "rm -rf lib/* && webpack --env build",
    "publish": "npm publish",
    "rebuild": "rm -rf node_modules && npm install && npm run build",
    "dev": "rm -rf lib/* && webpack --progress --colors --watch --env dev",
    "test": "mocha-webpack --colors --recursive -w --timeout 100000 --webpack-config test/config/webpack.config.test.js --require mock-local-storage --require test/config/setup.js --exit src/**/**/*.spec.js --exit ",
    "test:watch": "mocha-webpack --webpack-config webpack.config.test.js --watch --glob '*.spect.js' --recursive test"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/goatlab/corporate/goat/sockets-pusherrn.git"
  },
  "keywords": [
    "form.io",
    "js",
    "cordova",
    "browser"
  ],
  "author": "Tomas Castro",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/goatlab/corporate/goat/sockets-pusherrn.git/issues"
  },
  "homepage": "https://gitlab.com/goatlab/corporate/goat/sockets-pusherrn.git#readme",
  "dependencies": {
    "@goatlab/goatjs": "^1.1.7",
    "@stamp/it": "^1.1.0",
    "pusher-js": "^4.4.0"
  },
  "peerDependencies": {
    "react-native": "0.59.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "lerna": "^3.13.2",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.1"
  },
  "gitHead": "8f54ee13e8a7e027f293140f2ede13233926e625"
}
