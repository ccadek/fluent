{
  "name": "@goatlab/fluent-loopback",
  "version": "1.0.2",
  "description": "Fluent Connector for Loopback submissions",
  "main": "lib/@goatlab/fluent-loopback.min.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "rm -rf lib/* && webpack --env build",
    "rebuild": "rm -rf node_modules && npm install && npm run build",
    "dev": "rm -rf lib/* && webpack --progress --colors --watch --env dev",
    "test": "mocha-webpack --colors --recursive -w --timeout 100000 --webpack-config test/config/webpack.config.test.js --require mock-local-storage --require test/config/setup.js --exit src/**/**/*.spec.js --exit ",
    "test:watch": "mocha-webpack --webpack-config webpack.config.test.js --watch --glob '*.spect.js' --recursive test"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/goatlab/corporate/goat/fluent-loopback.git"
  },
  "keywords": [
    "form.io",
    "js",
    "cordova",
    "browser"
  ],
  "author": "Tomas Castro",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/goatlab/corporate/goat/fluent-loopback.git/issues"
  },
  "homepage": "https://gitlab.com/goatlab/corporate/goat/fluent-formio.git#readme",
  "dependencies": {
    "@goatlab/goat-fluent": "file:../Fluent",
    "await-to-js": "^2.1.1",
    "axios": "^0.18.0",
    "bluebird": "^3.5.3",
    "formiojs": "^3.19.9"
  }
}
