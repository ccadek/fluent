{
  "name": "@goatlab/goatjs",
  "version": "1.0.2",
  "description": "Formio interface for online and offline use",
  "main": "lib/@goatlab/goatjs.min.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "rm -rf lib/* && webpack --env build",
    "rebuild": "rm -rf node_modules && npm install && npm run build",
    "dev": "rm -rf lib/* && webpack --progress --colors --watch --env dev",
    "test": "mocha-webpack --colors --recursive -w --timeout 100000 --webpack-config test/config/webpack.config.test.js --require mock-local-storage --require test/config/setup.js --exit src/**/**/*.spec.js --exit ",
    "test:watch": "mocha-webpack --webpack-config webpack.config.test.js --watch --glob '*.spect.js' --recursive test"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/goatlabs/goatjs.git"
  },
  "keywords": [
    "form.io",
    "js",
    "cordova",
    "browser"
  ],
  "author": "Ignacio Cabrera",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/goatlabs/goatjs.git/issues"
  },
  "homepage": "https://gitlab.com/goatlabs/goatjs.git#readme",
  "dependencies": {
    "@goatlab/fluent-formio": "file:../Fluent-formio",
    "@goatlab/fluent-loopback": "file:../Fluent-loopback",
    "@goatlab/goat-fluent": "file:../Fluent",
    "@stamp/it": "^1.1.0",
    "await-to-js": "^2.0.1",
    "axios": "^0.18.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "bluebird": "^3.5.2",
    "btoa": "^1.2.1",
    "lokijs": "^1.5.5",
    "md5": "^2.2.1",
    "moment": "^2.22.2",
    "qs": "^6.7.0",
    "uuid": "^3.2.1"
  }
}
